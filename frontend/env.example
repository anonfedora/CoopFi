# CoopiFi Contract Configuration
# Copy this file to .env.local and fill in your values

# Deployed CoopiFi contract address on Starknet
NEXT_PUBLIC_CONTRACT_ADDRESS=0x0

# Starknet RPC endpoint
# For mainnet: https://starknet-mainnet.infura.io/v3/YOUR_API_KEY
# For testnet: https://starknet-goerli.infura.io/v3/YOUR_API_KEY
# For local: http://localhost:5050
NEXT_PUBLIC_RPC_URL=https://starknet-mainnet.infura.io/v3/YOUR_API_KEY

# Optional: Chain ID (mainnet: SN_MAINNET, testnet: SN_GOERLI)
NEXT_PUBLIC_CHAIN_ID=SN_MAINNET 